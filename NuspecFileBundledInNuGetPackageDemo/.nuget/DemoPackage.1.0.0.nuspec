<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Demo.Package</id>
    <version>1.0.0</version>
    <title>Demo Package for nuspec difficulties</title>
    <authors>eloekset</authors>
    <owners>My Company</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This NuGet package demonstrates the difficulties in deploying .nuspec files in a package.</description>
    <releaseNotes>Initial release.</releaseNotes>
    <copyright>My Company 2015</copyright>
    <tags>js, JavaScript</tags>
    <dependencies>
      <dependency id="jQuery" version="1.10.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\SimpleContent.txt" target="content\SimpleContent.txt" />
    <file src="..\TargetProjectNuSpecFile.nuspec" target="content\.nuget\NuSpecTemplateInWrongLocation.nuspec" />
    <file src="..\TargetProjectSpecTemplate.spec" target="content\.nuget\TemplateNuSpec.spec" />
  </files>
</package>